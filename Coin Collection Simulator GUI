eggs:CreateToggle("Auto Buy Cages 1", function(arg) --the (arg) is if the checkbox is toggled or not
    if arg then
        _G.a = true 
        while wait(0) and _G.a do
           local args = {
    [1] = "StarterCage",
}

workspace.petInfo:InvokeServer(unpack(args))
        end
    else
        _G.a = false
    end
end)


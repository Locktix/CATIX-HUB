--================== Eggs =======================--
local c = w3:CreateFolder("Eggs")

c:Toggle("Buy Eggs",function(bool)
	    shared.Eggs = bool
	end)
	c:Dropdown("Choose Eggs",{"Basic","Spotted","Tropical","Emoji","Carnival","Ancient","Ocean","Alien","Villian","Dragon","Arcade","Russo","Frost","Mystic","Spirit","Music","Abyssal","Minion","Minion 2","Frozen","Pirate","Dragon","11M Event"},true,function(mob) --Replace the Dropdown name with the selected one(A,B,C)
	    _G.EggsSelected = mob
	end)
spawn(function()
	while wait(0) do
		if shared.Eggs then
			if _G.EggsSelected == "Basic" then
			   local args = {
                [1] = {
                    ["EggType"] = "Egg1",

AutoTab:CreateToggle("Inf Cash", function(arg) --the (arg) is if the checkbox is toggled or not
    if arg then
        _G.Basic = true 
       while wait(0) do 
coins = 
local EnterCode = game:GetService("ReplicatedStorage").EnterCode
EnterCode:FireServer(
    coins,
    "Code1"
)
end
    else
        _G.Basic = false
    end
end)

--| Anti AFK | By Altix#3395
    local Virtual = game:service'VirtualUser'
    game:service'Players'.LocalPlayer.Idled:connect(function()
        Virtual:CaptureController()
        Virtual:ClickButton2(Vector2.new())
        wait(2)
    end)

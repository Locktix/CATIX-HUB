eggs:CreateToggle("Auto Buy 1K Shurikens eggs", function(arg) --the (arg) is if the checkbox is toggled or not
    if arg then
        _G.Basic = true 
        while wait(0) and _G.Basic do
         local args = {
    [1] = "Basic Egg",
    [2] = true
}

workspace.Debris.Events.OpenEgg:FireServer(unpack(args))


        end
    else
        _G.Basic = false
    end
end)


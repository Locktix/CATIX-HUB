eggs:CreateToggle("Auto Buy Dragon Egg", function(arg) --the (arg) is if the checkbox is toggled or not
    if arg then
        _G.Basic = true 
        while wait(0) and _G.Basic do
            local args = {
    [1] = 1,
    [2] = workspace.Incubators.NewDragonEgg.Data,
}

game:GetService("ReplicatedStorage").Remotes.BuyIncEgg:InvokeServer(unpack(args))

        end
    else
        _G.Basic = false
    end
end)

AutoTab:CreateButton("Destroy Gui :D", function() --you dont need "arg" for a button
game:GetService("CoreGui").uiui:Destroy()
end)


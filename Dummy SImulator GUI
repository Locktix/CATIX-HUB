AutoTab:CreateToggle("Auto Rebirths", function(arg) --the (arg) is if the checkbox is toggled or not
    if arg then
        _G.Basic = true 
        while wait(0) and _G.Basic do
           
local Event = game:GetService("ReplicatedStorage").Remotes.Rebirth
Event:FireServer()


        end
    else
        _G.Basic = false
    end
end)

